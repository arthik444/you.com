{
  "intents": {
    "PlanInfo": {
      "description": "User wants information about a specific health insurance plan",
      "required_entities": ["plan_name", "insurer", "year", "county", "age"],
      "optional_entities": ["income", "family_size"],
      "entity_order": ["plan_name", "insurer", "year", "county", "age"]
    },
    "CoverageDetail": {
      "description": "User wants details about what a plan covers",
      "required_entities": ["plan_name", "insurer", "year", "coverage_item", "subtype", "county"],
      "optional_entities": [],
      "entity_order": ["plan_name", "insurer", "year", "coverage_item", "subtype", "county"]
    },
    "ProviderNetwork": {
      "description": "User wants to know about doctors/hospitals in a plan's network",
      "required_entities": ["provider_name", "specialty", "plan_name", "insurer", "county"],
      "optional_entities": [],
      "entity_order": ["provider_name", "specialty", "plan_name", "insurer", "county"]
    },
    "Comparison": {
      "description": "User wants to compare multiple plans",
      "required_entities": ["plan_name", "insurer", "year", "county"],
      "optional_entities": ["age", "features"],
      "entity_order": ["plan_name", "insurer", "year", "county"]
    },
    "FAQ": {
      "description": "User has a general question about health insurance",
      "required_entities": ["topic"],
      "optional_entities": ["state"],
      "entity_order": ["topic"]
    },
    "News": {
      "description": "User wants recent news or updates about plans/insurers",
      "required_entities": ["topic", "year"],
      "optional_entities": ["insurer", "plan_name", "state"],
      "entity_order": ["topic", "year"]
    }
  },
  "entities": {
    "plan_name": {
      "type": "string",
      "description": "Name of the health insurance plan",
      "examples": ["Molina Silver 1 HMO", "Aetna Gold"],
      "question_template": "Which insurance plan are you interested in?",
      "use_dynamic_question": true
    },
    "insurer": {
      "type": "string",
      "description": "Insurance company name",
      "examples": ["Molina", "Aetna", "UnitedHealthcare"],
      "question_template": "Which insurance company are you asking about?",
      "use_dynamic_question": true
    },
    "year": {
      "type": "integer",
      "description": "Year of coverage",
      "examples": ["2024", "2025"],
      "question_template": "Which year are you interested in?",
      "use_dynamic_question": true
    },
    "county": {
      "type": "string",
      "description": "County name",
      "examples": ["Broward", "Miami-Dade", "Leon"],
      "question_template": "Which county do you live in?",
      "use_dynamic_question": true
    },
    "age": {
      "type": "integer",
      "description": "Age of the person",
      "examples": ["43", "65"],
      "question_template": "What is your age?",
      "use_dynamic_question": true
    },
    "coverage_item": {
      "type": "string",
      "description": "Specific coverage type",
      "examples": ["dental", "vision", "prescription drugs", "mental health"],
      "question_template": "Which coverage are you asking about?",
      "use_dynamic_question": true
    },
    "subtype": {
      "type": "string",
      "description": "Subtype or specific aspect of coverage",
      "examples": ["preventive care", "specialist visits"],
      "question_template": "What specific aspect of this coverage?",
      "use_dynamic_question": true
    },
    "provider_name": {
      "type": "string",
      "description": "Name of a doctor or hospital",
      "examples": ["Dr. Smith", "Memorial Hospital"],
      "question_template": "Which provider are you asking about?",
      "use_dynamic_question": true
    },
    "specialty": {
      "type": "string",
      "description": "Medical specialty",
      "examples": ["cardiology", "pediatrics", "dermatology"],
      "question_template": "What medical specialty?",
      "use_dynamic_question": true
    },
    "features": {
      "type": "string",
      "description": "Plan features to compare",
      "examples": ["premiums", "deductibles", "copays"],
      "question_template": "Which features would you like to compare?",
      "use_dynamic_question": true
    },
    "topic": {
      "type": "string",
      "description": "Topic or subject matter - any insurance concept or question",
      "examples": ["coinsurance", "deductible", "copay", "open enrollment", "subsidies", "qualifying life events", "HMO vs PPO", "out-of-pocket maximum", "premium"],
      "question_template": "What topic are you interested in?",
      "use_dynamic_question": true
    },
    "state": {
      "type": "string",
      "description": "State name",
      "examples": ["Florida", "Texas", "California"],
      "question_template": "Which state?",
      "use_dynamic_question": true
    },
    "income": {
      "type": "integer",
      "description": "Annual income",
      "examples": ["50000", "75000"],
      "question_template": "What is your approximate annual income?",
      "use_dynamic_question": true
    },
    "family_size": {
      "type": "integer",
      "description": "Number of people to be covered",
      "examples": ["1", "4"],
      "question_template": "How many people need coverage?",
      "use_dynamic_question": true
    },
    "zip_code": {
      "type": "string",
      "description": "ZIP code",
      "examples": ["33301", "90210"],
      "question_template": "What's your zip code?",
      "use_dynamic_question": true
    }
  }
}

